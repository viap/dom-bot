{
  "name": "dom-bot",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "copy-assets": "cp -r ./src/assets ./dist/assets",
    "build": "rm -rf ./dist && tspc && npm run copy-assets",
    "start": "node --es-module-specifier-resolution=node dist/index.js",
    "startInspect": "node --inspect --es-module-specifier-resolution=node dist/index.js",
    "restart:node": "npm run build && npm run start",
    "restart:dev": "tsx ./src",
    "dev": "nodemon",
    "lint": "eslint --fix-dry-run ./src"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@grammyjs/types": "^2.12.1",
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.4",
    "ts-patch": "^3.3.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.2",
    "typescript-transform-paths": "^3.5.2"
  },
  "dependencies": {
    "@grammyjs/conversations": "^1.1.1",
    "@grammyjs/menu": "^1.1.3",
    "@grammyjs/runner": "^1.0.4",
    "@grammyjs/storage-mongodb": "^2.1.1",
    "axios": "^1.4.0",
    "dotenv": "^16.0.3",
    "grammy": "^1.19.2",
    "jwt-decode": "^4.0.0",
    "mongoose": "^7.0.1",
    "socket.io-client": "^4.7.2",
    "telegram-inline-calendar": "^2.1.0"
  }
}
